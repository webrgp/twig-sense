{
  "grammars": [
    {
      "name": "@twig-sense/twig",
      "camelcase": "TwigSenseTwig",
      "scope": "source.twig",
      "path": ".",
      "file-types": [
        "twig"
      ],
      "injection-regex": "twig"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for Twig templates",
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-@twig-sense/twig"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
